<p id="notice"><%= notice %></p>

<h1>Seances</h1>

<table>
  <thead>
    <tr>
      <th>Start date</th>
      <th>Movie</th>
      <th>Room</th>
      <th>Threed</th>
      <th>Lector</th>
      <th>Subtitle</th>
      <th>Dubbing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seances.each do |seance| %>
      <tr>
        <td><%= seance.start_date %></td>
        <td><%= seance.movie_id %></td>
        <td><%= seance.room_id %></td>
        <td><%= seance.threed %></td>
        <td><%= seance.lector %></td>
        <td><%= seance.subtitle %></td>
        <td><%= seance.dubbing %></td>
        <td><%= link_to 'Show', seance %></td>
        <td><%= link_to 'Edit', edit_seance_path(seance) %></td>
        <td><%= link_to 'Destroy', seance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seance', new_seance_path %>
